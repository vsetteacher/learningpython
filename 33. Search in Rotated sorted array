        count = 0
        rindex = {}
        for i in nums:
            rindex[i] = count
            count += 1
        
        return rindex[target] if target in rindex else -1
 #########################################################
 class Solution:
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        return nums.index(target) if target in nums else -1
 #########################################################
 
