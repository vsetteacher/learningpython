import itertools
class Solution:
    def combinationSum3(self, k: 'int', n: 'int') -> 'List[List[int]]':
        nums = [i for i in range(1,10)]
        res = []
        l = list(itertools.combinations(nums,k))
        for j in l:
            if sum(j) == n:
                res.append(j)
        return res
