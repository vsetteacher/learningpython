class Solution:
    def searchRange(self, nums: 'List[int]', target: 'int') -> 'List[int]':
        start = end = -1 
        for i in range(len(nums)):
            if nums[i] == target:
                if start == -1:
                    start = end = i
                else:
                    end = i
       
        return [start,end]
#############################################################################
# narrow down from two end
class Solution:
    def searchRange(self, nums: 'List[int]', target: 'int') -> 'List[int]':
        start = end = -1 
        for i in range(len(nums)):
            if nums[i] == target:
                start = i
                break
        
        for j in range(len(nums)-1, -1, -1):
            if nums[j] == target:
                end = j
                break
       
        return [start,end]
