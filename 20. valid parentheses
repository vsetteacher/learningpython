class Solution:
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        
        stack = []
        mapping = {')':'(', '}':'{', ']':'['}
        
        for char in s:
            if char in mapping:
                if stack:
                    top = stack.pop()
                else:
                    top = ''
                if mapping[char] != top:
                    return False
            else:
                stack.append(char)
        return not stack       
                        
#utilize FIFO behavior of stack to achieve
